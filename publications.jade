extends layout

block vars
	-var pageid = "publications"

mixin listpubs(pubs)
	each pub,index in pubs
		li
			a(href='http://dx.doi.org/'+pub["doi"], target="_blank") #{pub["title"]}
			br
			|  !{pub["authors"]}. 
			em #{pub["journal"]}
			| , #{pub["datepage"]}. 
			| [
			a(href="/pdf/"+(pubs.length-index)+".pdf", target="_blank") PDF
			| ]
			.toc
				img(src="images/pubs/toc"+(pubs.length-index)+".gif", height="150")

block title
	title Publications
	meta(name='description',content='A complete list of publications by Kyle Broaders and his lab.')

